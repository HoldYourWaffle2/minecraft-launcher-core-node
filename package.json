{
  "name": "ts-minecraft",
  "version": "5.2.1",
  "main": "./dest/index.js",
  "types": "./dest/index.d.ts",
  "description": "Propabaliy all the things I want for a minecraft launcher. Targeting ElectronJs environment!",
  "engines": {
    "node": ">=10.0"
  },
  "devDependencies": {
    "@microsoft/rush": "^5.10.0",
    "tslint": "^5.14.0",
    "typescript": "^3.4.1"
  },
  "scripts": {
    "update": "rush update",
    "test": "npm run build && node ./node_modules/mocha/bin/mocha dest/tests --timeout 10000",
    "build": "npm run clean && tslint -c tslint.json 'src/libs/*.ts' && tslint -c src/tests/tslint.json 'src/test/*.ts' && tsc",
    "clean": "node clean.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ci010/ts-minecraft.git"
  },
  "author": "cijhn@hotmail.com",
  "keywords": [
    "minecraft",
    "typescript",
    "minecraft-launcher",
    "nodejs",
    "electron"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ci010/ts-minecraft/issues"
  },
  "homepage": "https://github.com/ci010/ts-minecraft#readme"
}